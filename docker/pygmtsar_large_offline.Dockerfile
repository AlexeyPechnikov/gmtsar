# place the Dockerfile into your data files directory
FROM mobigroup/pygmtsar:latest

ENV ORIG=./
ENV DATA=data_desc

# download example notebooks
RUN mkdir tmp \
&&  cd tmp \
&&  svn export https://github.com/mobigroup/YamchiDam/trunk/notebooks \
&&  cd .. \
&&  mv tmp/notebooks/*.ipynb . \
&&  rm -rf tmp

# split layers
